                },
                {
                  "text": "4.6."
                },
                {
                  "text": "Non concern\u00e9."
                },
                {
                  "text": "6.2."
                },
                {
                  "text": "Non concern\u00e9."
                }
              ],
              "parametrization": {
                "potential_inapplicabilities": [],
                "potential_modifications": [],
                "warnings": []
              },
              "reference_str": "Annexe I 2.",
              "sections": [
                {
